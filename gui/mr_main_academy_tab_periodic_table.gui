							text = "[GetPlayer.GetCustom('mendelejew_periodic_table_name_choser_type')]"
							parentanchor = center
							position = { 0 -4 }
							autoresize = yes
							max_width = 1000
							elide = right
							using = fontsize_xxl
						}
					}

					######################### Before Invention of Periodic Table #########################

					### Table head ###
					flowcontainer = {
						visible = "[Not( GetScriptedGui('mr_periodic_table_created_sgui').IsShown( GuiScope.SetRoot( GetPlayer.MakeScope ).End ) )]"
						position = { 0 40 }
						spacing = 20
						margin_top = 10
						margin_left = 20

						#Number
						textbox = {
							align = left|nobaseline
							using = fontsize_large
							elide = right
							size = { 100 40 }
							raw_text = "MENDELEJEW_ELEMENT_NUMBER"
						}
						#Name
						textbox = {
							align = left|nobaseline
							using = fontsize_large
							elide = right
							size = { 180 40 }
							raw_text = "MENDELEJEW_ELEMENT_NAME"
						}
						#Status
						textbox = {
							align = left|nobaseline
							using = fontsize_large
							size = { 120 40 }
							elide = right
							raw_text = "MENDELEJEW_ELEMENT_STATUS"
						}
						#Atomic Mass
						textbox = {
							align = right|nobaseline
							using = fontsize_large
							size = { 120 40 }
							elide = left
							raw_text = "MENDELEJEW_ELEMENT_ATOMIC_MASS"

